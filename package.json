{
  "type": "module",
  "name": "openclaw-cursor-orchestrator",
  "version": "1.0.0",
  "description": "Orchestration service connecting OpenClaw and Cursor Cloud Agents for automated app development",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "node scripts/check-no-openai.mjs",
    "build": "prisma generate && npm run build:server && npm run build:web",
    "build:server": "tsc",
    "build:web": "cd web && npm install && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:web": "cd web && npm run dev",
    "typecheck": "tsc --noEmit",
    "test": "NODE_ENV=test node --import tsx --test test/**/*.test.ts",
    "e2e": "npx tsx scripts/e2e-user-simulation.ts",
    "e2e:sim": "npx tsx scripts/e2e-simulations.ts",
    "e2e:orch": "npx tsx scripts/e2e-orchestration-sim.ts",
    "e2e:fix": "npx tsx scripts/e2e-fix-flow-sim.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "prisma": "^7.4.1",
    "stripe": "^20.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
